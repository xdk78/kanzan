{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "rules": {
    "semicolon": [true, "never"],
    "max-line-length": false,
    "variable-name": false,
    "import-name": false,
    "object-shorthand-properties-first": false,
    "object-literal-key-quotes": false,
    "quotemark": [true, "single"],
    "align": false
  }
}
